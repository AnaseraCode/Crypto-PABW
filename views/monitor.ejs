<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Monitor</title>
</head>
<body>
  <h1>Monitor</h1>
  <div class="charts-container">
    <div class="chart-row">
      <!-- Widgets untuk 3 koin pertama -->
      <% for(let i=0; i<3; i++) { %>
        <div class="chart-widget" id="chart_<%= selectedCoins[i] %>"></div>
      <% } %>
    </div>
    <div class="chart-row">
      <!-- Widgets untuk 3 koin berikutnya -->
      <% for(let i=3; i<6; i++) { %>
        <div class="chart-widget" id="chart_<%= selectedCoins[i] %>"></div>
      <% } %>
    </div>
  </div>
</body>
</html>